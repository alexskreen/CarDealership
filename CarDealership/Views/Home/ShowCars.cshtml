@using CarDealership.Models;

<h1>Car Dealership</h1>

@if (Model.Count == 0)
{
    <p>There are no results</p>
}
<ul>
    @foreach (Car car in Model)
    {
        <li>Year: @car.Year Make: @car.Make Model: @car.Model  Price: @car.Price Miles: @car.Miles</li>
    }
</ul>

<a href= '/cars/new'> Add a new car</a>